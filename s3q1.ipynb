   "source": [
    "import string\n",
    "\n",
    "def remove_punctuation(text):\n",
    "    return text.translate(str.maketrans('', '', string.punctuation))\n",
    "\n",
    "text = input(\"Enter a string: \")\n",
    "result = remove_punctuation(text)\n",
    "print(\"String without punctuation:\", result)\n"
   ]
