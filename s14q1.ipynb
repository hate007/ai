   "source": [
    "def sort_sentence(sentence):\n",
    "    words = sentence.split()\n",
    "    words.sort()\n",
    "    return ' '.join(words)\n",
    "\n",
    "sentence = input(\"Enter a sentence: \")\n",
    "print(sort_sentence(sentence))\n"
   ]
